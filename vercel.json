{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo-uri",
    "REDIS_URI": "@redis-uri",
    "SECRET_KEY": "@secret-key",
    "REFRESH_TOKEN_SECRET": "@refresh-token-secret-key",
    "PORT": "8000",
    "NODE_ENV": "production"
  }
}

